{
    "name": "@pubgcorp/semantic-release-helm",
    "version": "1.3.2",
    "description": "Helm plugin for [semantic-release](https://github.com/semantic-release/semantic-release)",
    "main": "./index.js",
    "repository": {
        "type": "git",
        "url": "git@git.projectbro.com:Devops/semantic-release-helm.git"
    },
    "author": "JungHun Kim <mnthe@pubg.com>",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "CHANGELOG.md"
                }
            ],
            "@semantic-release/npm",
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "package.json",
                        "CHANGELOG.md"
                    ]
                }
            ],
            [
                "@semantic-release/gitlab",
                {
                    "gitlabUrl": "https://git.projectbro.com"
                }
            ]
        ]
    },
    "devDependencies": {
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/commit-analyzer": "^8.0.1",
        "@semantic-release/git": "^9.0.0",
        "@semantic-release/gitlab": "^6.0.9",
        "@semantic-release/npm": "^7.1.3",
        "@semantic-release/release-notes-generator": "^9.0.2",
        "semantic-release": "^17.4.3"
    },
    "dependencies": {
        "execa": "^4.1.0",
        "js-yaml": "^4.1.0",
        "semver": "^7.3.5"
    }
}
