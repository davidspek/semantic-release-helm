    "@semantic-release/release-notes-generator": "^14.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.2",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
